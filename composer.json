{
  "name": "yapro/symfony-loggerext",
  "type": "library",
  "description": "Useful logger functionality in symfony",
  "keywords":    ["php", "error", "library", "logger", "log"],
  "license": "proprietary",
  "version": "1.0",
  "authors": [
    {
      "name": "Lebedenko Nikolay",
      "email": "totx@ya.ru",
      "role": "Developer",
      "homepage": "http://yapro.ru"
    }
  ],
  "require": {
    "php": ">=7.4",
    "symfony/monolog-bridge": "*",
    "symfony/dependency-injection": "*"
  },
  "autoload": {
    "psr-4": {
      "YaPro\\SymfonyLoggerExt\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "YaPro\\SymfonyLoggerExt\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  }
}
